function accountSelect(accountType,level1,level2,accountName) {
        $("#account_all").hide();
     //   $("#income_expense").hide();
        $(".hidden_details").hide();
        $("#oth_acc_type").val(accountType);
        $("#oth_acc_l1").val(level1);
        $("#oth_acc_l2").val(level2);
        $("#oth_acc_name").val(accountName);
        $("#other_accounts").show();
}


