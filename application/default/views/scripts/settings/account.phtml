function account(value) {
    if(value==1) {
        $(".assets_active").show();
        $(".liability_active").show();
        $(".income_active").show();
        $(".expense_active").show();
        $(".equity_active").show();
    } else if(value==2) {
        $(".assets_active").show();
        $(".liability_active").show();
        $(".income_active").hide();
        $(".expense_active").hide();
        $(".equity_active").show();
    }  else if(value==3) {
        $(".assets_active").hide();
        $(".liability_active").hide();
        $(".income_active").show();
        $(".expense_active").show();
        $(".equity_active").hide();
    }
}

